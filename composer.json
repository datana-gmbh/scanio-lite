{
    "name": "datana-gmbh/scanio-lite",
    "description": "Scanio-Lite is a lightweight version of Scanio.",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-filter": "*",
        "ext-pdo_pgsql": "*",
        "api-platform/core": "^3.2.12",
        "datana-gmbh/doctrine-postgres-public-schema-listener": "^1.2.0",
        "datana-gmbh/logz-io-handler": "^1.7",
        "doctrine/collections": "^1.8",
        "doctrine/dbal": "^3.8.0",
        "doctrine/doctrine-bundle": "^2.11.1",
        "doctrine/doctrine-migrations-bundle": "^3.3.0",
        "doctrine/event-manager": "^2.0",
        "doctrine/migrations": "^3.7.2",
        "doctrine/orm": "^2.17.4",
        "doctrine/persistence": "^3.2",
        "easycorp/easyadmin-bundle": "^4.8.12",
        "knplabs/knp-menu": "^3.4",
        "knplabs/knp-menu-bundle": "^3.3",
        "league/flysystem": "^3.23.1",
        "league/flysystem-azure-blob-storage": "^3.23.1",
        "league/flysystem-bundle": "^3.3.2",
        "microsoft/azure-storage-blob": "^1.5.4",
        "nelmio/cors-bundle": "^2.4.0",
        "oskarstark/enum-helper": "^1.2",
        "oskarstark/symfony-http-responder": "^1.5",
        "oskarstark/trimmed-non-empty-string": "^1.1.1",
        "psr/log": "^3.0",
        "spatie/dropbox-api": "^1.22",
        "spatie/flysystem-dropbox": "^3.0.1",
        "symfony/asset": "^7.0",
        "symfony/console": "^7.0.2",
        "symfony/dependency-injection": "^7.0.2",
        "symfony/doctrine-bridge": "^7.0.2",
        "symfony/dotenv": "^7.0.2",
        "symfony/event-dispatcher": "^7.0.2",
        "symfony/event-dispatcher-contracts": "^3.4",
        "symfony/expression-language": "^7.0.2",
        "symfony/filesystem": "^7.0",
        "symfony/finder": "^7.0",
        "symfony/flex": "^2.4.3",
        "symfony/form": "^7.0.1",
        "symfony/framework-bundle": "^7.0.2",
        "symfony/http-foundation": "^7.0",
        "symfony/http-kernel": "^7.0.2",
        "symfony/mime": "^7.0",
        "symfony/monolog-bundle": "^3.10",
        "symfony/options-resolver": "^7.0",
        "symfony/password-hasher": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/routing": "^7.0.2",
        "symfony/security-bundle": "^7.0.2",
        "symfony/security-core": "^7.0.1",
        "symfony/security-http": "^7.0.1",
        "symfony/serializer": "^7.0.2",
        "symfony/string": "^7.0.2",
        "symfony/translation": "^7.0.2",
        "symfony/translation-contracts": "^3.4.1",
        "symfony/twig-bridge": "^7.0.2",
        "symfony/twig-bundle": "^7.0",
        "symfony/uid": "^7.0",
        "symfony/validator": "^7.0.2",
        "symfony/var-dumper": "^7.0.2",
        "symfony/webpack-encore-bundle": "^2.1.1",
        "symfony/yaml": "^7.0",
        "thecodingmachine/safe": "^2.5",
        "twig/twig": "^3.8.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0.1",
        "datana-gmbh/doctrine-type-testcases": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.5.1",
        "ergebnis/composer-normalize": "^2.41.1",
        "ergebnis/php-cs-fixer-config": "^6.20",
        "ergebnis/test-util": "^1.6",
        "fakerphp/faker": "^1.23.1",
        "icanhazstring/composer-unused": "^0.8.11",
        "infection/infection": "^0.27.9",
        "mtdowling/jmespath.php": "^2.7.0",
        "phpstan/extension-installer": "^1.3.1",
        "phpstan/phpstan": "^1.10.56",
        "phpstan/phpstan-deprecation-rules": "^1.1.4",
        "phpstan/phpstan-doctrine": "^1.3.59",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "phpstan/phpstan-strict-rules": "^1.5.2",
        "phpstan/phpstan-symfony": "^1.3.7",
        "phpstan/phpstan-webmozart-assert": "^1.2.4",
        "phpunit/phpunit": "^9.6.16",
        "rector/rector": "^0.19.1",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/debug-bundle": "^7.0",
        "symfony/maker-bundle": "^1.52.0",
        "symfony/phpunit-bridge": "^7.0.2",
        "symfony/web-profiler-bundle": "^7.0.2",
        "thecodingmachine/phpstan-safe-rule": "^1.2",
        "vincentlanglet/twig-cs-fixer": "^2.4",
        "zenstruck/browser": "^1.7",
        "zenstruck/console-test": "^1.5",
        "zenstruck/foundry": "^1.36.1"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php54": "*",
        "symfony/polyfill-php55": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "endroid/installer": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true
        },
        "optimize-autoloader": false,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.0.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
